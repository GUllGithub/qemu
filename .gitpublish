# The rhev profile must be first, because it is the default
[gitpublishprofile "rhev"]
base = rhev7/master-2.1.2
prefix = RHEV-7.1 qemu-kvm-rhev PATCH
to = rhvirt-patches@redhat.com
suppresscc = all
message = True

# Special project, Acadia, for aarm64 architecture.
[gitpublishprofile "acadia"]
base = rhev7/master-2.1.2
prefix = RHELSA qemu-kvm-rhev PATCH
to = rhvirt-patches@redhat.com
cc = virt-arm@redhat.com
suppresscc = all
message = True
